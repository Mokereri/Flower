CREATE DATABASE molvin_db;
USE molvin_db;

-- Users Table
CREATE TABLE users_table (
    user_id INT AUTO_INCREMENT PRIMARY KEY,
    username VARCHAR(100),
    email VARCHAR(100) UNIQUE,
    phone_number VARCHAR(20),
    location_latitude DECIMAL(10, 8),
    location_longitude DECIMAL(11, 8),
    browser_used VARCHAR(100),
    clicks INT DEFAULT 0,
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP
);

-- Sample Data for Users Table
INSERT INTO users_table (username, email, phone_number, location_latitude, location_longitude, browser_used, clicks)
VALUES
('Alex Baker', 'baker@gmail.com', '+1234567890', -1.292066, 36.821945, 'Chrome', 45),
('Mwita Mwita', 'mwita@gmail.com', '+9876543210', 51.507351, -0.127758, 'Firefox', 32),
('Robi Maswi', 'robimaswi@gmail.com', '+4567891230', 40.712776, -74.005974, 'Safari', 20);

-- Products Table
CREATE TABLE products_table (
    product_id INT AUTO_INCREMENT PRIMARY KEY,
    product_name VARCHAR(100),
    description TEXT,
    price DECIMAL(10, 2),
    stock INT,
    image_url VARCHAR(255),
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP
);
SELECT * FROM products_table;
