-- Sales Table
CREATE TABLE sales_table (
    sale_id INT AUTO_INCREMENT PRIMARY KEY,
    user_id INT,
    product_id INT,
    quantity INT,
    total_price DECIMAL(10, 2),
    sale_date TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    FOREIGN KEY (user_id) REFERENCES users_table(user_id),
    FOREIGN KEY (product_id) REFERENCES products_table(product_id)
);

-- Sample Data for Sales Table
INSERT INTO sales_table (user_id, product_id, quantity, total_price)
VALUES
(1, 1, 10, 200.00),
(2, 2, 5, 75.00),
(3, 3, 2, 50.00);

-- Customers Table
CREATE TABLE customers_table (
    customer_id INT AUTO_INCREMENT PRIMARY KEY,
    product_id INT,
    location_latitude DECIMAL(10, 8),
    location_longitude DECIMAL(11, 8),
    quantity INT,
    purchase_date TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    FOREIGN KEY (product_id) REFERENCES products_table(product_id)
);

CREATE TABLE subscribers (
  id INT AUTO_INCREMENT PRIMARY KEY,
  email VARCHAR(255) UNIQUE NOT NULL,
  created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP
);

SELECT * FROM subscribers;

-- Sample Data for Customers Table
INSERT INTO customers_table (product_id, location_latitude, location_longitude, quantity)
VALUES
(1, -1.292066, 36.821945, 12),
(2, 51.507351, -0.127758, 8),
(3, 40.712776, -74.005974, 5);

-- Orders Table 
CREATE TABLE orders_table (
    order_id INT AUTO_INCREMENT PRIMARY KEY,
    user_id INT,
    total_amount DECIMAL(10, 2),
    order_date TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    status ENUM('Pending', 'Completed', 'Cancelled'),
    FOREIGN KEY (user_id) REFERENCES users_table(user_id)
);

-- Sample Data for Orders Table
INSERT INTO orders_table (user_id, total_amount, status)
VALUES
(1, 200.00, 'Completed'),
(2, 75.00, 'Pending'),
(3, 50.00, 'Cancelled');
